    "php": ">=5.3.0",
    "mf2/mf2": ">=v0.3.0",
    "ml/json-ld": "1.0.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.3.2"
  },
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net",
      "homepage": "https://jkphl.is",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "joschi@kuphal.net",
    "issues": "https://github.com/jkphl/micrometa/issues",
    "source": "https://github.com/jkphl/micrometa"
  },
  "keywords": [
    "html",
    "micro",
    "parser",
    "meta",
    "microformats",
    "microdata"
